{
	"name": "@nais/ds-svelte",
	"version": "0.0.6",
	"homepage": "https://nais.github.io/ds-svelte",
	"repository": "https://github.com/nais/ds-svelte.git",
	"license": "MIT",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && npm run package",
		"preview": "vite preview",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"storybook": "storybook dev -p 6006 --no-open",
		"build-storybook": "storybook build",
		"storybook-docs": "storybook dev --docs",
		"generate-icons": "./hack/generate-icons.sh"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js"
		},
		"./icons": {
			"types": "./dist/icons/index.d.ts",
			"svelte": "./dist/icons/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"svelte": "^3.59.1"
	},
	"devDependencies": {
		"@navikt/aksel-icons": "^3.0.1",
		"@navikt/ds-tailwind": "^3.0.1",
		"@storybook/addon-a11y": "^7.0.10",
		"@storybook/addon-essentials": "^7.0.10",
		"@storybook/addon-interactions": "^7.0.10",
		"@storybook/addon-links": "^7.0.10",
		"@storybook/addon-storysource": "^7.0.10",
		"@storybook/addon-svelte-csf": "^3.0.2",
		"@storybook/blocks": "^7.0.10",
		"@storybook/svelte": "^7.0.10",
		"@storybook/sveltekit": "^7.0.10",
		"@storybook/testing-library": "^0.1.0",
		"@sveltejs/adapter-auto": "^2.0.1",
		"@sveltejs/kit": "^1.16.3",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.59.5",
		"@typescript-eslint/parser": "^5.59.5",
		"autoprefixer": "^10.4.14",
		"eslint": "^8.40.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.12",
		"eslint-plugin-svelte3": "^4.0.0",
		"normalize.css": "^8.0.1",
		"postcss": "^8.4.23",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.0",
		"publint": "^0.1.11",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"storybook": "^7.0.10",
		"svelte": "^3.59.1",
		"svelte-check": "^3.3.2",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.0",
		"typescript": "^5.0.4",
		"vite": "^4.3.5"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"locally-unique-id-generator": "^0.1.5",
		"@navikt/ds-css": "^3.0.1",
		"@navikt/ds-tokens": "^3.0.1"
	}
}
